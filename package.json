{
  "name": "suntech",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite --host --open",
    "serve-development": "vite --mode development --host --open",
    "serve-staging": "vite --mode staging --host --open",
    "serve-production": "vite --mode production --host --open",
    "build-development": "run-p type-check build-only-development",
    "build-staging": "run-p type-check build-only-staging",
    "build-production": "run-p type-check build-only-production",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only-development": "vite build --mode development",
    "build-only-staging": "vite build --mode staging",
    "build-only-production": "vite build --mode production",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@jsonforms/core": "3.1.0",
    "@jsonforms/vue": "3.1.0",
    "@jsonforms/vue-vanilla": "3.1.0",
    "vue": "^3.2.47",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.10"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.15.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  }
}
